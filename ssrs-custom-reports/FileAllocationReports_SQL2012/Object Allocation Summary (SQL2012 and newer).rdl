  <DataSets>
    <DataSet Name="DBLocal">
      <Query>
        <DataSourceName>LocalServer</DataSourceName>
        <CommandText>SELECT DB_NAME() AS DBName, CAST(ServerProperty('ServerName') AS nvarchar(1000)) AS ServerName</CommandText>
      </Query>
      <Fields>
        <Field Name="DBName">
          <DataField>DBName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ServerName">
          <DataField>ServerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="PageAllocation">
      <Query>
        <DataSourceName>LocalServer</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ObjectType">
            <Value>=Parameters!ObjectTypeName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ObjectName">
            <Value>=Parameters!ObjectName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DatabaseName">
            <Value>=Parameters!DatabaseName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SET NOCOUNT ON;
DECLARE @IndexId int = NULL, @TableId int = NULL;

IF @ObjectType = 'Index'
BEGIN
	SELECT @TableId = object_id
	FROM sys.indexes
	WHERE name = @ObjectName

	IF @@ROWCOUNT &lt;&gt; 1 RAISERROR(N'Unable to determine table to which index "%s" belongs', 16, 1, @ObjectName);

	SET @IndexId = INDEXPROPERTY(@TableId, @ObjectName, 'IndexID');
END
ELSE IF @ObjectType = 'Table'
BEGIN
	SET @TableId = OBJECT_ID(@ObjectName);
END
ELSE
BEGIN
	RAISERROR(N'Object Type "%s" is not supported', 16, 1, @ObjectType);
END

IF OBJECT_ID('tempdb..#tmp') IS NOT NULL DROP TABLE #tmp;
CREATE TABLE #tmp
(
  file_id smallint NOT NULL
, page_type int NOT NULL
, page_type_desc varchar(256) NOT NULL
, free_bytes bigint NULL
, used_bytes bigint NULL
, object_id int NULL
, index_id int NULL
, partition_id int NULL
);

PRINT CONVERT(nvarchar(25), GETDATE(), 121) + N' - starting insert into #tmp';

INSERT INTO #tmp
SELECT
  p.allocated_page_file_id
, ISNULL(p.page_type, 0)
, ISNULL(p.page_type_desc, 'EMPTY')
, free_bytes = SUM(ISNULL((100 - p.page_free_space_percent) / 100.0 * 8192, 0))
, used_bytes = SUM(ISNULL(p.page_free_space_percent / 100.0 * 8192, 8192))
, p.object_id
, p.index_id
, p.partition_id
FROM sys.dm_db_database_page_allocations(DB_ID(@DatabaseName),@TableId,@IndexId,default,'DETAILED') AS p
GROUP BY
  p.allocated_page_file_id
, ISNULL(p.page_type, 0)
, ISNULL(p.page_type_desc, 'EMPTY')
, p.object_id
, p.index_id
, p.partition_id

PRINT CONVERT(nvarchar(25), GETDATE(), 121) + N' - creating clustered index on #tmp';

CREATE UNIQUE CLUSTERED INDEX IX_pageId ON #tmp (object_id ASC, index_id ASC, partition_id ASC, page_type ASC, file_id ASC) WITH(DATA_COMPRESSION = PAGE);

PRINT CONVERT(nvarchar(25), GETDATE(), 121) + N' - output raw #tmp';

SELECT
  t.file_id
, t.page_type
, t.page_type_desc
, t.free_bytes
, t.used_bytes
, t.object_id
, t.index_id
, t.partition_id
, schema_name = OBJECT_SCHEMA_NAME(t.object_id)
, object_name = ob.[name]
, index_name = ix.[name]
FROM #tmp AS t
LEFT JOIN sys.all_objects AS ob ON t.object_id = ob.object_id
LEFT JOIN sys.indexes AS ix ON t.object_id = ix.object_id AND t.index_id = ix.index_id
ORDER BY t.file_id, t.object_id, t.index_id, t.partition_id</CommandText>
      </Query>
